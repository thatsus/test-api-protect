{
    "name": "thatsus/test-api-protect",
    "description": "Avoid hitting third party API's by forcing their client classes to be mocked in tests",
    "authors": [
        {
            "name": "Dan Kuck-Alvarez",
            "email": "dankuck@gmail.com",
            "homepage": "http://dankuck.github.io/",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "ThatsUs\\": "src/ThatsUs"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "license": "MIT"
}
